{
  "name": "",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/functions/*.js",
  "scripts": {
    "build": "tsc && npm run copy:sql",
    "copy:sql": "mkdir -p dist/src/db/migrations && cp src/db/migrations/*.sql dist/src/db/migrations/",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "prestart": "npm run clean && npm run build",
    "start": "func start",
    "dev": "tsc -w & func start",
    "test": "echo \"No tests yet...\""
  },
  "dependencies": {
    "@azure/functions": "^4.0.0",
    "@nktkas/hyperliquid": "^0.27.1",
    "@types/pg": "^8.15.6",
    "joi": "^18.0.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "22.x",
    "azure-functions-core-tools": "^4.x",
    "rimraf": "^5.0.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=16.9.0"
  }
}
